msgid "cg_thirdPersonRange"
msgstr "<value> The distance that the player model appears from the camera when in third person view.\n"
"^6Note: You must enable sv_cheats \"1\" and cg_thirdPerson \"1\" for this command to work."

#. Console/cg
msgid "cg_viewBob"
msgstr "<0/1> Enabled head bobbing.\n"
"^6Disable: cg_viewBob \"0\"\n"
"^6Enable: cg_viewBob \"1\""

#. Console/cg
msgid "cg_viewSize"
msgstr "<40-100> Viewport size.\n"
"^6Fullscreen: cg_viewSize \"100\"\n"
"^6Note: If cg_viewSize is less than 100, then a black frame will fill-in the remaining percentage of viewable area."

#. Console/cg
msgid "cg_voiceChats"
msgstr "<0/1> Enables voice commands (also known as 'VSAYS').\n"
"^6Disable: cg_voiceChats \"0\"\n"
"^6Enable: cg_voiceChats \"1\""

#. Console/cg
msgid "cg_volume_announcer"
msgstr "<0-2.0>	Changes the volume of the announcer.\n"
"^6Disable: cg_volume_announcer \"0\""

#. Console/cg
msgid "cg_volume_effects"
msgstr "<0-2.0>	Changes the volume of the effects.\n"
"^6Disable: cg_volume_effects \"0\""

#. Console/cg
msgid "cg_volume_hitsound"
msgstr "<0-2.0>	Changes the volume of the hitsound.\n"
"^6Disable: cg_volume_hitsound \"0\""

#. Console/cg
msgid "cg_volume_players"
msgstr "<0-2.0>	Changes the volume of player sounds.\n"
"^6Disable: cg_volume_players \"0\""

#. Console/cg
msgid "cg_volume_voicechats"
msgstr "<0.0-2.0> Changes the volume of voice commands.\n"
"^6Disable: cg_volume_voicechats \"0\""

#. Console/cg
msgid "cg_weaponAutoswitch"
msgstr "<0-2> Enables auto switching of weapons.\n"
"^6Disable: cg_weaponAutoswitch \"0\"\n"
"^6Switch when you don't have the just picked weapon: cg_weaponAutoswitch \"1\"\n"
"^6Switch when the new weapon improves the selected weapon: cg_weaponAutoswitch \"2\""

#. Console/cg
msgid "cg_weaponFlashes"
msgstr "<0/2> Enables weapon flashes.\n"
"^6Disable: cg_weaponFlashes \"0\"\n"
"^6Enable: cg_weaponFlashes \"2\"\n"
"^6Note: You must also enable cg_gun \"1\" for weapon muzzle flashes."

#. Console/cg
msgid "cg_weaponlist"
msgstr "<0/1> If enabled, the weapon list will be displayed on your HUD.\n"
"^6Disable: cg_weaponlist \"0\"\n"
"^6Enable: cg_weaponlist \"1\""
